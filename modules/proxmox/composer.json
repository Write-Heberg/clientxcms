{
    "name": "clientxcms/proxmox",
    "type": "clientxcms-plugin",
    "description": "Pluck your Proxmox with CLIENTXCMS",
    "autoload": {
        "psr-4": {
            "App\\Modules\\Proxmox\\": "src/"
        }
    },
    "require": {
        "php": ">=8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "platform-check": false
    },
    "providers": [{
        "provider": "App\\Modules\\Proxmox\\ProxmoxServiceProvider"
    }],
    "viewspaths":[
        {
            "viewspaths": "views"
        }
    ]
}
